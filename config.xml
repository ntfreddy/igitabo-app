<?xml version="1.0" encoding="utf-8"?>
<widget id="com.ntfreddy.igitaboapp" version="2022.8.7" android-versionCode="9" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" ios-CFBundleVersion="9">
  <preference name="android-minSdkVersion" value="19"/>
  <preference name="DisallowOverscroll" value="true"/>
  <hook src="hooks/hookers.js" type="before_prepare"/>
  <hook src="hooks/beforedep.js" type="before_deploy"/>
  <name>igitabo</name>
  <description>
        Book library app
    </description>
  <author email="ntfreddy@gmail.com" href="http://cordova.io">
        Apache Cordova Team
    </author>
  <content src="index.html"/>
  <plugin name="cordova-plugin-whitelist" spec="1"/>
  <access origin="*"/>
  <allow-intent href="http://*/*"/>
  <allow-intent href="https://*/*"/>
  <allow-intent href="tel:*"/>
  <allow-intent href="sms:*"/>
  <allow-intent href="mailto:*"/>
  <allow-intent href="geo:*"/>
  <platform name="android">
    <icon background="res/icon/android/mipmap-mdpi/ic_launcher_adaptive_back.png" density="mdpi" foreground="res/icon/android/mipmap-mdpi/ic_launcher_adaptive_fore.png"/>
    <icon background="res/icon/android/mipmap-hdpi/ic_launcher_adaptive_back.png" density="hdpi" foreground="res/icon/android/mipmap-hdpi/ic_launcher_adaptive_fore.png"/>
    <icon background="res/icon/android/mipmap-xhdpi/ic_launcher_adaptive_back.png" density="xhdpi" foreground="res/icon/android/mipmap-xhdpi/ic_launcher_adaptive_fore.png"/>
    <icon background="res/icon/android/mipmap-xxhdpi/ic_launcher_adaptive_back.png" density="xxhdpi" foreground="res/icon/android/mipmap-xxhdpi/ic_launcher_adaptive_fore.png"/>
    <icon background="res/icon/android/mipmap-xxxhdpi/ic_launcher_adaptive_back.png" density="xxxhdpi" foreground="res/icon/android/mipmap-xxxhdpi/ic_launcher_adaptive_fore.png"/>
    <allow-intent href="market:*"/>
  </platform>
  <platform name="ios">
    <splash src="res/screen/ios/Default@2x~universal~anyany.png"/>
    <allow-intent href="itms:*"/>
    <allow-intent href="itms-apps:*"/>
  </platform>
  <allow-navigation href="*"/>
</widget>
